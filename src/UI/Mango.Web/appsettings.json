{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceUrls": {
    "ProductsApi": "https://products-api",
    "ShoppingCartApi": "https://shoppingcart-api",
    "CouponsApi": "https://coupons-api",
    "IdentityApp": "https://identity-app",
    "OrdersApi": "https://orders-api"
  },
  "OpenIdConnect": {
    "Authority": "https://identity-app",
    "ClientId": "mango",
    "ClientSecret": "secret",
    "ResponseType": "code",
    "Scopes": [
      "mango"
    ],
    "GetClaimsFromUserInfoEndpoint": true,
    "SaveTokens": true,
    "CookieExpireMinutes": 10
  }
}